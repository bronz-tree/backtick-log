{
    "name": "backtick-log",
    "displayName": "Backtick Log",
    "description": "Quick simple logging with backticks",
    "version": "0.0.2",
    "main": "./src/extension.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "engines": {
        "vscode": "^1.60.0"
    },
    "keywords": [],
    "author": "bronze.treee@gmail.com",
    "publisher": "bronzetree",
    "license": "ISC",
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.addLog",
                "title": "Add Log",
                "category": "Extension"
            }
        ],
        "keybindings": [
            {
                "command": "extension.addLog",
                "key": "ctrl+alt+l",
                "when": "editorTextFocus",
                "os": "win"
            },
            {
                "command": "extension.addLog",
                "key": "option+alt+l",
                "when": "editorTextFocus",
                "os": "mac"
            }
        ]
    },
    "devDependencies": {
        "vscode": "^1.1.37"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bronz-tree/backtick-log"
    }
}
